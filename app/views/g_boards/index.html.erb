<h1>Listing g_boards</h1>

<table>
  <thead>
    <tr>
      <th>Graphic</th>
      <th>Image</th>
      <th>Place</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @g_boards.each do |g_board| %>
      <tr>
        <td><%= g_board.graphic %></td>
        <td><%= g_board.image %></td>
        <td><%= g_board.place %></td>
        <td><%= link_to 'Show', g_board %></td>
        <td><%= link_to 'Edit', edit_g_board_path(g_board) %></td>
        <td><%= link_to 'Destroy', g_board, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New G board', new_g_board_path %>
